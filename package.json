  "license": "MIT",
  "scripts": {
    "dev": "node fix-windows.js && concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "cd client && npm run dev",
    "dev:server": "nodemon --exec node --loader ts-node/esm server/index.ts",
    "build": "npm install && cd client && npm install && npm run build && cd .. && esbuild server/index.ts server/optimize.ts server/performance.ts server/server-wrapper.ts --platform=node --packages=external --bundle --format=esm --outdir=dist && mkdir -p dist/client && cp -r client/dist/* dist/client/ && node optimize-assets.js",
    "optimize": "node optimize-assets.js",
    "start": "NODE_ENV=production node dist/index.js",
    "start:server": "NODE_ENV=production node dist/index.js"
  },
